.at-curtain {
  display: block;
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: rgba(0, 0, 0, 0.3);
  z-index: 1080;
  /* elements */
  /* modifiers */
}
.at-curtain__container {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  position: relative;
  margin: 0 auto;
  width: 80%;
  height: 100%;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
      -ms-flex-pack: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  opacity: 1;
}
.at-curtain__body {
  position: relative;
  width: 100%;
}
.at-curtain__btn-close {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  position: absolute;
  width: 56rpx;
  height: 56rpx;
  margin-left: -28rpx;
  left: 50%;
  bottom: -80rpx;
  -webkit-box-align: center;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
      -ms-flex-pack: center;
          justify-content: center;
  border: 2PX solid #FFF;
  border-radius: 50%;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  z-index: 1080;
}
.at-curtain__btn-close::before, .at-curtain__btn-close::after {
  content: "";
  position: absolute;
  top: 50%;
  left: 50%;
  display: inline-block;
  width: 34rpx;
  height: 2PX;
  border-radius: 1PX;
  background: #FFF;
}
.at-curtain__btn-close::before {
  -webkit-transform: translate3d(-50%, -50%, 0) rotate(45deg);
          transform: translate3d(-50%, -50%, 0) rotate(45deg);
}
.at-curtain__btn-close::after {
  -webkit-transform: translate3d(-50%, -50%, 0) rotate(-45deg);
          transform: translate3d(-50%, -50%, 0) rotate(-45deg);
}
.at-curtain__btn-close--top {
  margin-left: -28rpx;
  top: -80rpx;
  left: 50%;
  bottom: auto;
}
.at-curtain__btn-close--top-left {
  top: -80rpx;
  left: 0;
  bottom: auto;
}
.at-curtain__btn-close--top-right {
  top: -80rpx;
  left: auto;
  right: 0;
  bottom: auto;
}
.at-curtain__btn-close--bottom {
  margin-left: -28rpx;
  bottom: -80rpx;
  left: 50%;
}
.at-curtain__btn-close--bottom-left {
  bottom: -80rpx;
  left: 0;
}
.at-curtain__btn-close--bottom-right {
  bottom: -80rpx;
  left: auto;
  right: 0;
}
.at-curtain--closed {
  visibility: hidden;
}